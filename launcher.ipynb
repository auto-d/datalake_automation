   "metadata": {
    "tags": []
   },
   "outputs": [],
   "source": [
    "bucket = s3.Bucket(bucket_name)\n",
    "print(f\"S3 Bucket {bucket_name} contents: \")\n",
    "for ob in bucket.objects.all():\n",
    "    print(f\" - {ob.key}\")"
   ]
